{
        "id": "org.gnome.eog",
        "version": "3.26.0-1.fc27",
        "runtime": "org.fedoraproject.GnomePlatform",
        "runtime-version": "27",
        "sdk": "org.fedoraproject.GnomeSdk",
        "rename-desktop-file": "eog.desktop",
        "rename-appdata-file": "eog.appdata.xml",
        "rename-icon": "eog",
        "command": "eog",
        "tags": ["Viewer"],
        "finish-args": ["--command=eog",
                        "--filesystem=home",
                        "--share=ipc",
                        "--socket=x11",
                        "--socket=wayland",
                        "--socket=session-bus",
                        "--filesystem=~/.config/dconf:ro",
                        "--filesystem=xdg-run/dconf",
                        "--talk-name=ca.desrt.dconf",
                        "--env=DCONF_USER_CONFIG_DIR=.config/dconf"]
}
