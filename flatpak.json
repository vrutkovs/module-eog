{
        "id": "org.gnome.eog",
        "version": " 	3.26.0-1.fc27",
        "runtime": "org.fedoraproject.GnomePlatform",
        "runtime-version": "27",
        "sdk": "org.fedoraproject.GnomeSdk",
        "command": "eog",
        "tags": ["Viewer"],
        "finish-args": ["--filesystem=host",
                        "--share=ipc",
